import itertools
def Permutations (n, k):
    example=list(itertools.combinations([x for x in range(1,3+1)], 2))
    print("List of permutations: "+str(example) +"\n"+ "Is number of permutations Even or Odd: "+ ("EVEN" if len(example)%2==0 else "ODD"))
    return 'Using Bitwise And Operator: EVEN' if ~n & k else 'Using Bitwise And Operator: ODD'
